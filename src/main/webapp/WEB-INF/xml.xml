<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
	
	<servlet>
     	<servlet-name>InsertServlet</servlet-name>
     	<servlet-class>musicStore.InsertServlet</servlet-class>
     </servlet>
     
     
     <servlet-mapping>
     	<servlet-name>InsertServlet</servlet-name>
     	<url-pattern>/InsertServlet</url-pattern>
     </servlet-mapping>
     
     
     
     <servlet>
     	<servlet-name>GetAllServelet</servlet-name>
     	<servlet-class>/musicStore.GetAllServelet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>GetAllServelet</servlet-name>
     	<url-pattern>/GetAllServelet</url-pattern>
     </servlet-mapping>
     
     
     <servlet>
     	<servlet-name>UpdateServlet</servlet-name>
     	<servlet-class>/musicStore.UpdateServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UpdateServlet</servlet-name>
     	<url-pattern>/UpdateServlet</url-pattern>
     </servlet-mapping>
     
     <!-- user System -->
     <servlet>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<servlet-class>/com.dropbeatzuser.servlet.UserInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<url-pattern>/UserInsertServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>profileServlet</servlet-name>
     	<servlet-class>/com.dropbeatzuser.servlet.profileServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>profileServlet</servlet-name>
     	<url-pattern>/profileServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>LoginServlet</servlet-name>
     	<servlet-class>/com.dropbeatzuser.servlet.LoginServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>LoginServlet</servlet-name>
     	<url-pattern>/LoginServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>UpdateProfileServlet</servlet-name>
     	<servlet-class>/com.dropbeatzuser.servlet.UpdateProfileServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UpdateProfileServlet</servlet-name>
     	<url-pattern>/UpdateProfileServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>AccountDeleteServlet</servlet-name>
     	<servlet-class>/com.dropbeatzuser.servlet.AccountDeleteServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>AccountDeleteServlet</servlet-name>
     	<url-pattern>/AccountDeleteServlet</url-pattern>
     </servlet-mapping>
     


</web-app>